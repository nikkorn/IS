{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "0.1.0",
	"command": "gradlew",
	"isShellCommand": true,
	"showOutput": "always",
	"suppressTaskName": true,
	"tasks": [
		{
			"taskName": "vscode",
			"args": ["vscode"]
		},
		{
			"taskName": "dependencies",
			"args": ["dependencies"]
		},
		{
			"taskName": "build",
			"args": ["build"],
			"isBuildCommand": true
		},
		{
			"taskName": "test",
			"args": ["test"],
			"isTestCommand": true
		},
		{
			"taskName": "run",
			"args": ["run"]
		}
	]
}